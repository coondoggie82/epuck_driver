<launch>
  <!-- Run AMCL -->
  <!-- <include file="$(find amcl)/examples/amcl_diff.launch" /> -->

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find epuck_driver)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find epuck_driver)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find epuck_driver)/params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find epuck_driver)/params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find epuck_driver)/params/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find epuck_driver)/params/base_global_planner_params.yaml" command="load" />
    <remap from="/cmd_vel" to="/mobile_base/cmd_vel" />
  </node>
</launch>
